{
  "name": "darwinia-storage",
  "version": "1.1.2",
  "types": "./index.d.ts",
  "description": "A library to help fetch substrate storages via evm json-rpc `eth.call`",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "tsc",
    "genpangolin": "CHAIN=pangolin ENDPOINT=https://pangolin-rpc.darwinia.network ts-node generator/generator.ts",
    "gencrab": "CHAIN=crab ENDPOINT=https://darwiniacrab-rpc.dwellir.com ts-node generator/generator.ts",
    "genpangoro": "CHAIN=pangoro ENDPOINT=https://pangoro-rpc.darwinia.network ts-node generator/generator.ts",
    "gendarwinia": "CHAIN=darwinia ENDPOINT=https://darwinia-rpc.dwellir.com ts-node generator/generator.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darwinia-network/darwinia-storage.git"
  },
  "keywords": [
    "substrate",
    "darwinia",
    "storage",
    "evm"
  ],
  "author": "Aki Wu",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/darwinia-network/darwinia-storage/issues"
  },
  "homepage": "https://github.com/darwinia-network/darwinia-storage#readme",
  "type": "commonjs",
  "devDependencies": {
    "@types/ejs": "^3.1.1",
    "axios": "^1.2.1",
    "ejs": "^3.1.8",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@polkadot/types": "^9.9.4",
    "ethers": "^5.7.2"
  }
}
