{
  "name": "darwinia-js-sdk",
  "version": "1.2.20",
  "description": "A library to help fetch substrate storages via evm json-rpc `eth.call`",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "rm -rf dist && tsc",
    "mypublsih": "npm run prepublish && npm publish",
    "genpangolin2": "CHAIN=pangolin2 ENDPOINT=https://cors.kahub.in/http://g1.dev.darwinia.network:10000 ts-node generator/generator.ts",
    "genpangolin": "CHAIN=pangolin ENDPOINT=https://pangolin-rpc.darwinia.network ts-node generator/generator.ts",
    "gencrab": "CHAIN=crab ENDPOINT=https://crab-rpc.darwinia.network ts-node generator/generator.ts",
    "genpangoro": "CHAIN=pangoro ENDPOINT=https://pangoro-rpc.darwinia.network ts-node generator/generator.ts",
    "gendarwinia": "CHAIN=darwinia ENDPOINT=https://rpc.darwinia.network ts-node generator/generator.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darwinia-network/darwinia-storage.git"
  },
  "keywords": [
    "substrate",
    "darwinia",
    "storage",
    "evm"
  ],
  "author": "Aki Wu",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/darwinia-network/darwinia-storage/issues"
  },
  "homepage": "https://github.com/darwinia-network/darwinia-storage#readme",
  "type": "commonjs",
  "devDependencies": {
    "@types/ejs": "^3.1.1",
    "@types/jest": "^29.2.5",
    "axios": "^1.2.1",
    "ejs": "^3.1.8",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@polkadot/types": "^9.9.4",
    "ethers": "^5.7.2"
  }
}
